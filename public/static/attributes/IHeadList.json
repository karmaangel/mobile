{
  "classId": "idm.component.mobile.iheadlist",
  "comName": "头像列表",
  "className": "IHeadList",
  "comType": "common",
  "comLangue": "vue",
  "compositeAttr": [
      {
        "type": "input",
        "layoutType": "inline",
        "text": "唯一标识",
        "bindKey": "ctrlId",
        "disabled": true,
        "default": "@[packageid]",
        "desc": "",
        "helpUrl": ""
      },
      {
        "type": "group",
        "text": "基本属性",
        "desc": "提供设置组件能达到展示效果的基本属性设置信息",
        "children": [
          {
            "type": "group",
            "text": "右侧图标",
            "desc": "右侧图标",
            "helpUrl": "",
            "children": [
                {
                  "type": "switch",
                  "layoutType": "inline",
                  "text": "图标显示",
                  "desc":"图标是否显示",
                  "bindKey": "showIcon",
                  "default": true
                },
                {
                    "type": "iconSelect",
                    "layoutType": "inline",
                    "text": "图标",
                    "bindKey": "titleIconClass",
                    "default": ""
                },
                {
                    "type": "colorPicker",
                    "layoutType": "inline",
                    "desc": "设置图标颜色，会覆盖主题色",
                    "text": "图标颜色",
                    "bindKey": "titleIconFontColor"
                },
                {
                    "type": "inputNumber",
                    "layoutType": "inline",
                    "text": "图标大小",
                    "bindKey": "titleIconFontSize",
                    "default": 18
                }
            ]
          },
          {
            "type": "group",
            "text": "个人图标",
            "desc": "个人图标",
            "helpUrl": "",
            "children": [
                {
                  "type": "switch",
                  "layoutType": "inline",
                  "text": "图标显示",
                  "desc":"图标是否显示",
                  "bindKey": "authorshowIcon",
                  "default": true
                },
                {
                  "type": "inputNumber",
                  "layoutType": "inline",
                  "text": "图标大小",
                  "bindKey": "authorIconFontSize",
                  "default": 18
                },
                {
                    "type": "iconSelect",
                    "layoutType": "inline",
                    "text": "man图标",
                    "bindKey": "authorIconmaxClass",
                    "default": ""
                },
                {
                  "type": "colorPicker",
                  "layoutType": "inline",
                  "desc": "设置图标颜色，会覆盖主题色",
                  "text": "man图标颜色",
                  "bindKey": "authorIconmaxColor"
                },
                {
                  "type": "iconSelect",
                  "layoutType": "inline",
                  "text": "girl图标",
                  "bindKey": "authorIcongirlClass",
                  "default": ""
                },
                {
                  "type": "colorPicker",
                  "layoutType": "inline",
                  "desc": "设置图标颜色，会覆盖主题色",
                  "text": "girl图标颜色",
                  "bindKey": "authorIcongirlColor"
                }
            ]
          }
        ]
      },
      {
        "type": "group",
        "text": "外层容器样式",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "children": [
          {
            "type": "colorPicker",
            "layoutType": "inline",
            "text": "背景色",
            "bindKey": "bgColor",
            "placeholder": "请选择颜色",
            "default": {}
          },
          {
            "type": "box",
            "layoutType": "block",
            "text": "内外边距",
            "bindKey": "box",
            "default": {
              "marginTopVal": "",
              "marginRightVal": "10px",
              "marginBottomVal": "",
              "marginLeftVal": "10px",
              "paddingTopVal": "",
              "paddingRightVal": "",
              "paddingBottomVal": "",
              "paddingLeftVal": ""
            }
          }
        ]
      },
      {
        "type": "group",
        "text": "内层容器样式",
        "desc": "点击？查看样式设置用法指南",
        "helpUrl": "",
        "children": [
          {
            "type": "box",
            "layoutType": "block",
            "text": "列表边距",
            "bindKey": "liBox",
            "default": {
              "marginTopVal": "",
              "marginRightVal": "",
              "marginBottomVal": "",
              "marginLeftVal": "",
              "paddingTopVal": "",
              "paddingRightVal": "20px",
              "paddingBottomVal": "",
              "paddingLeftVal": "20px"
            }
          },
          {
            "type": "box",
            "layoutType": "block",
            "text": "分割边距",
            "bindKey": "lipadding",
            "default": {
              "marginTopVal": "",
              "marginRightVal": "",
              "marginBottomVal": "",
              "marginLeftVal": "",
              "paddingTopVal": "20px",
              "paddingRightVal": "",
              "paddingBottomVal": "20px",
              "paddingLeftVal": ""
            }
          },
          {
            "type": "border",
            "layoutType": "block",
            "text": "分割线",
            "bindKey": "border",
            "default": {
              "border": {
                "bottom": {
                  "colors": {
                    "hex": "#ccc"
                  },
                  "style": "solid",
                  "visible": true,
                  "width": 1,
                  "widthUnit": "px"
                },
                "left": {
                  "colors": {}
                },
                "right": {
                  "colors": {}
                },
                "top": {
                  "colors": {}
                }
              },
              "radius": {
                "leftBottom": {},
                "leftTop": {},
                "rightBottom": {},
                "rightTop": {}
              }
            }
          },
          {
            "type": "font",
            "layoutType": "block",
            "text": "标题字体",
            "bindKey": "titleStyle",
            "default": {
                "fontColors": {
                    "hex": "#000000"
                },
                "fontSize": 14,
                "fontSizeUnit": "px"
            }
          },
          {
            "type": "font",
            "layoutType": "block",
            "text": "描述字体",
            "bindKey": "descStyle",
            "default": {
                "fontColors": {
                    "hex": "#000000"
                },
                "fontSize": 14,
                "fontSizeUnit": "px"
            }
          }
        ]
      },
      {
        "type": "group",
        "text": "高级",
        "desc": "点击？查看高级设置用法指南",
        "children": [
          {
            "type": "treeSelect",
            "layoutType": "inline",
            "text": "数据源",
            "bindKey": "dataSource",
            "dataSourceUrl":"/ctrl/dataSource/getDatasourceByGroup"
          },
          {
            "type": "group",
            "text": "页面数据绑定",
            "desc": "页面数据绑定",
            "children": [
              {
                "type": "textarea",
                "layoutType": "inline",
                "text": "头像字段",
                "bindKey": "ImgInterface"
              },
              {
                "type": "textarea",
                "layoutType": "inline",
                "text": "标题字段",
                "bindKey": "titleInterface"
              },
              {
                "type": "textarea",
                "layoutType": "inline",
                "text": "描述字段",
                "bindKey": "descInterface"
              },
              {
                "type": "textarea",
                "layoutType": "inline",
                "text": "性别字段",
                "bindKey": "sexInterface",
                "default": "type == '1'"
              }
            ]
          },
          {
            "type": "table",
            "text": "标签字段",
            "bindKey": "tagTable",
            "tabText": "页签[index]",
            "children": [
              {
                "type": "input",
                "layoutType": "inline",
                "text": "tab标识",
                "bindKey": "key"
              },
              {
                "type": "input",
                "layoutType": "inline",
                "text": "标签字段",
                "bindKey": "tab",
                "desc": "取数据：为false, 标签字段是：填写对应数据的参数 eg: xxx。如果是判断: true, 标签字段是: 参数名='1' eg: xxx='1'"
              },
              {
                "type": "switch",
                "layoutType": "inline",
                "text": "判断还是取数据",
                "bindKey": "defaultActiveKey",
                "default": false
              }
            ]
          },
          {
            "type": "actionSelect",
            "layoutType": "block",
            "text": "右侧图标自定义函数",
            "bindKey": "clickFunction",
            "desc": "点击右侧图标自定义函数{...url参数,customParam:自定义的,activeKey,allKey}",
            "multiple": true
          }
        ]
      }
  ]
}
    